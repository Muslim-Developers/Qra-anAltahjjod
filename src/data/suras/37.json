{
  "id": 37,
  "name": "سورة الصافات ",
  "parts": [
    {
      "id": 1,
      "color": "#ddf0fe",
      "from-to": ["1", "1"],
      "location": "s37from1to1",
      "content": "وَالصَّافَّاتِ صَفًّا ﴿1﴾"
    },
    {
      "id": 2,
      "color": "#ddf0fe",
      "from-to": ["2", "2"],
      "location": "s37from2to2",
      "content": "فَالزَّاجِرَاتِ زَجْرًا ﴿2﴾"
    },
    {
      "id": 3,
      "color": "#ddf0fe",
      "from-to": ["3", "3"],
      "location": "s37from3to3",
      "content": "فَالتَّالِيَاتِ ذِكْرًا ﴿3﴾"
    },
    {
      "id": 4,
      "color": "#ddf0fe",
      "from-to": ["4", "4"],
      "location": "s37from4to4",
      "content": "إِنَّ إِلَٰهَكُمْ لَوَاحِدٌ ﴿4﴾"
    },
    {
      "id": 5,
      "color": "#ddf0fe",
      "from-to": ["5", "5"],
      "location": "s37from5to5",
      "content": "رَبُّ السَّمَاوَاتِ وَالْأَرْضِ وَمَا بَيْنَهُمَا وَرَبُّ الْمَشَارِقِ ﴿5﴾"
    },
    {
      "id": 6,
      "color": "#ddf0fe",
      "from-to": ["6", "6"],
      "location": "s37from6to6",
      "content": "إِنَّا زَيَّنَّا السَّمَاءَ الدُّنْيَا بِزِينَةٍ الْكَوَاكِبِ ﴿6﴾"
    },
    {
      "id": 7,
      "color": "#ddf0fe",
      "from-to": ["7", "7"],
      "location": "s37from7to7",
      "content": "وَحِفْظًا مِنْ كُلِّ شَيْطَانٍ مَارِدٍ ﴿7﴾"
    },
    {
      "id": 8,
      "color": "#ddf0fe",
      "from-to": ["8", "8"],
      "location": "s37from8to8",
      "content": "لَا يَسَّمَّعُونَ إِلَى الْمَلَإِ الْأَعْلَىٰ وَيُقْذَفُونَ مِنْ كُلِّ جَانِبٍ ﴿8﴾"
    },
    {
      "id": 9,
      "color": "#ddf0fe",
      "from-to": ["9", "9"],
      "location": "s37from9to9",
      "content": "دُحُورًا ۖ وَلَهُمْ عَذَابٌ وَاصِبٌ ﴿9﴾"
    },
    {
      "id": 10,
      "color": "#ddf0fe",
      "from-to": ["10", "10"],
      "location": "s37from10to10",
      "content": "إِلَّا مَنْ خَطِفَ الْخَطْفَةَ فَأَتْبَعَهُ شِهَابٌ ثَاقِبٌ ﴿10﴾"
    },
    {
      "id": 11,
      "color": "#f4c29e",
      "from-to": ["11", "11"],
      "location": "s37from11to11",
      "content": "فَاسْتَفْتِهِمْ أَهُمْ أَشَدُّ خَلْقًا أَمْ مَنْ خَلَقْنَا ۚ إِنَّا خَلَقْنَاهُمْ مِنْ طِينٍ لَازِبٍ ﴿11﴾"
    },
    {
      "id": 12,
      "color": "#f4c29e",
      "from-to": ["12", "12"],
      "location": "s37from12to12",
      "content": "بَلْ عَجِبْتَ وَيَسْخَرُونَ ﴿12﴾"
    },
    {
      "id": 13,
      "color": "#f4c29e",
      "from-to": ["13", "13"],
      "location": "s37from13to13",
      "content": "وَإِذَا ذُكِّرُوا لَا يَذْكُرُونَ ﴿13﴾"
    },
    {
      "id": 14,
      "color": "#f4c29e",
      "from-to": ["14", "14"],
      "location": "s37from14to14",
      "content": "وَإِذَا رَأَوْا آيَةً يَسْتَسْخِرُونَ ﴿14﴾"
    },
    {
      "id": 15,
      "color": "#f4c29e",
      "from-to": ["15", "15"],
      "location": "s37from15to15",
      "content": "وَقَالُوا إِنْ هَٰذَا إِلَّا سِحْرٌ مُبِينٌ ﴿15﴾"
    },
    {
      "id": 16,
      "color": "#f4c29e",
      "from-to": ["16", "16"],
      "location": "s37from16to16",
      "content": "أَإِذَا مِتْنَا وَكُنَّا تُرَابًا وَعِظَامًا أَإِنَّا لَمَبْعُوثُونَ ﴿16﴾"
    },
    {
      "id": 17,
      "color": "#f4c29e",
      "from-to": ["17", "17"],
      "location": "s37from17to17",
      "content": "أَوَآبَاؤُنَا الْأَوَّلُونَ ﴿17﴾"
    },
    {
      "id": 18,
      "color": "#f4c29e",
      "from-to": ["18", "18"],
      "location": "s37from18to18",
      "content": "قُلْ نَعَمْ وَأَنْتُمْ دَاخِرُونَ ﴿18﴾"
    },
    {
      "id": 19,
      "color": "#f4c29e",
      "from-to": ["19", "19"],
      "location": "s37from19to19",
      "content": "فَإِنَّمَا هِيَ زَجْرَةٌ وَاحِدَةٌ فَإِذَا هُمْ يَنْظُرُونَ ﴿19﴾"
    },
    {
      "id": 20,
      "color": "#f4c29e",
      "from-to": ["20", "20"],
      "location": "s37from20to20",
      "content": "وَقَالُوا يَا وَيْلَنَا هَٰذَا يَوْمُ الدِّينِ ﴿20﴾"
    },
    {
      "id": 21,
      "color": "#f4c29e",
      "from-to": ["21", "21"],
      "location": "s37from21to21",
      "content": "هَٰذَا يَوْمُ الْفَصْلِ الَّذِي كُنْتُمْ بِهِ تُكَذِّبُونَ ﴿21﴾"
    },
    {
      "id": 22,
      "color": "#fedac7",
      "from-to": ["22", "22"],
      "location": "s37from22to22",
      "content": "احْشُرُوا الَّذِينَ ظَلَمُوا وَأَزْوَاجَهُمْ وَمَا كَانُوا يَعْبُدُونَ ﴿22﴾"
    },
    {
      "id": 23,
      "color": "#fedac7",
      "from-to": ["23", "23"],
      "location": "s37from23to23",
      "content": "مِنْ دُونِ اللَّهِ فَاهْدُوهُمْ إِلَىٰ صِرَاطِ الْجَحِيمِ ﴿23﴾"
    },
    {
      "id": 24,
      "color": "#fedac7",
      "from-to": ["24", "24"],
      "location": "s37from24to24",
      "content": "وَقِفُوهُمْ ۖ إِنَّهُمْ مَسْئُولُونَ ﴿24﴾"
    },
    {
      "id": 25,
      "color": "#fedac7",
      "from-to": ["25", "25"],
      "location": "s37from25to25",
      "content": "مَا لَكُمْ لَا تَنَاصَرُونَ ﴿25﴾"
    },
    {
      "id": 26,
      "color": "#fedac7",
      "from-to": ["26", "26"],
      "location": "s37from26to26",
      "content": "بَلْ هُمُ الْيَوْمَ مُسْتَسْلِمُونَ ﴿26﴾"
    },
    {
      "id": 27,
      "color": "#fedac7",
      "from-to": ["27", "27"],
      "location": "s37from27to27",
      "content": "وَأَقْبَلَ بَعْضُهُمْ عَلَىٰ بَعْضٍ يَتَسَاءَلُونَ ﴿27﴾"
    },
    {
      "id": 28,
      "color": "#fedac7",
      "from-to": ["28", "28"],
      "location": "s37from28to28",
      "content": "قَالُوا إِنَّكُمْ كُنْتُمْ تَأْتُونَنَا عَنِ الْيَمِينِ ﴿28﴾"
    },
    {
      "id": 29,
      "color": "#fedac7",
      "from-to": ["29", "29"],
      "location": "s37from29to29",
      "content": "قَالُوا بَلْ لَمْ تَكُونُوا مُؤْمِنِينَ ﴿29﴾"
    },
    {
      "id": 30,
      "color": "#fedac7",
      "from-to": ["30", "30"],
      "location": "s37from30to30",
      "content": "وَمَا كَانَ لَنَا عَلَيْكُمْ مِنْ سُلْطَانٍ ۖ بَلْ كُنْتُمْ قَوْمًا طَاغِينَ ﴿30﴾"
    },
    {
      "id": 31,
      "color": "#fedac7",
      "from-to": ["31", "31"],
      "location": "s37from31to31",
      "content": "فَحَقَّ عَلَيْنَا قَوْلُ رَبِّنَا ۖ إِنَّا لَذَائِقُونَ ﴿31﴾"
    },
    {
      "id": 32,
      "color": "#fedac7",
      "from-to": ["32", "32"],
      "location": "s37from32to32",
      "content": "فَأَغْوَيْنَاكُمْ إِنَّا كُنَّا غَاوِينَ ﴿32﴾"
    },
    {
      "id": 33,
      "color": "#fedac7",
      "from-to": ["33", "33"],
      "location": "s37from33to33",
      "content": "فَإِنَّهُمْ يَوْمَئِذٍ فِي الْعَذَابِ مُشْتَرِكُونَ ﴿33﴾"
    },
    {
      "id": 34,
      "color": "#fedac7",
      "from-to": ["34", "34"],
      "location": "s37from34to34",
      "content": "إِنَّا كَذَٰلِكَ نَفْعَلُ بِالْمُجْرِمِينَ ﴿34﴾"
    },
    {
      "id": 35,
      "color": "#fedac7",
      "from-to": ["35", "35"],
      "location": "s37from35to35",
      "content": "إِنَّهُمْ كَانُوا إِذَا قِيلَ لَهُمْ لَا إِلَٰهَ إِلَّا اللَّهُ يَسْتَكْبِرُونَ ﴿35﴾"
    },
    {
      "id": 36,
      "color": "#fedac7",
      "from-to": ["36", "36"],
      "location": "s37from36to36",
      "content": "وَيَقُولُونَ أَئِنَّا لَتَارِكُو آلِهَتِنَا لِشَاعِرٍ مَجْنُونٍ ﴿36﴾"
    },
    {
      "id": 37,
      "color": "#fedac7",
      "from-to": ["37", "37"],
      "location": "s37from37to37",
      "content": "بَلْ جَاءَ بِالْحَقِّ وَصَدَّقَ الْمُرْسَلِينَ ﴿37﴾"
    },
    {
      "id": 38,
      "color": "#fedac7",
      "from-to": ["38", "38"],
      "location": "s37from38to38",
      "content": "إِنَّكُمْ لَذَائِقُو الْعَذَابِ الْأَلِيمِ ﴿38﴾"
    },
    {
      "id": 39,
      "color": "#fedac7",
      "from-to": ["39", "39"],
      "location": "s37from39to39",
      "content": "وَمَا تُجْزَوْنَ إِلَّا مَا كُنْتُمْ تَعْمَلُونَ ﴿39﴾"
    },
    {
      "id": 40,
      "color": "#b1d1c1",
      "from-to": ["40", "40"],
      "location": "s37from40to40",
      "content": "إِلَّا عِبَادَ اللَّهِ الْمُخْلَصِينَ ﴿40﴾"
    },
    {
      "id": 41,
      "color": "#b1d1c1",
      "from-to": ["41", "41"],
      "location": "s37from41to41",
      "content": "أُولَٰئِكَ لَهُمْ رِزْقٌ مَعْلُومٌ ﴿41﴾"
    },
    {
      "id": 42,
      "color": "#b1d1c1",
      "from-to": ["42", "42"],
      "location": "s37from42to42",
      "content": "فَوَاكِهُ ۖ وَهُمْ مُكْرَمُونَ ﴿42﴾"
    },
    {
      "id": 43,
      "color": "#b1d1c1",
      "from-to": ["43", "43"],
      "location": "s37from43to43",
      "content": "فِي جَنَّاتِ النَّعِيمِ ﴿43﴾"
    },
    {
      "id": 44,
      "color": "#b1d1c1",
      "from-to": ["44", "44"],
      "location": "s37from44to44",
      "content": "عَلَىٰ سُرُرٍ مُتَقَابِلِينَ ﴿44﴾"
    },
    {
      "id": 45,
      "color": "#b1d1c1",
      "from-to": ["45", "45"],
      "location": "s37from45to45",
      "content": "يُطَافُ عَلَيْهِمْ بِكَأْسٍ مِنْ مَعِينٍ ﴿45﴾"
    },
    {
      "id": 46,
      "color": "#b1d1c1",
      "from-to": ["46", "46"],
      "location": "s37from46to46",
      "content": "بَيْضَاءَ لَذَّةٍ لِلشَّارِبِينَ ﴿46﴾"
    },
    {
      "id": 47,
      "color": "#b1d1c1",
      "from-to": ["47", "47"],
      "location": "s37from47to47",
      "content": "لَا فِيهَا غَوْلٌ وَلَا هُمْ عَنْهَا يُنْزَفُونَ ﴿47﴾"
    },
    {
      "id": 48,
      "color": "#b1d1c1",
      "from-to": ["48", "48"],
      "location": "s37from48to48",
      "content": "وَعِنْدَهُمْ قَاصِرَاتُ الطَّرْفِ عِينٌ ﴿48﴾"
    },
    {
      "id": 49,
      "color": "#b1d1c1",
      "from-to": ["49", "49"],
      "location": "s37from49to49",
      "content": "كَأَنَّهُنَّ بَيْضٌ مَكْنُونٌ ﴿49﴾"
    },
    {
      "id": 50,
      "color": "#b1acca",
      "from-to": ["50", "50"],
      "location": "s37from50to50",
      "content": "فَأَقْبَلَ بَعْضُهُمْ عَلَىٰ بَعْضٍ يَتَسَاءَلُونَ ﴿50﴾"
    },
    {
      "id": 51,
      "color": "#b1acca",
      "from-to": ["51", "51"],
      "location": "s37from51to51",
      "content": "قَالَ قَائِلٌ مِنْهُمْ إِنِّي كَانَ لِي قَرِينٌ ﴿51﴾"
    },
    {
      "id": 52,
      "color": "#b1acca",
      "from-to": ["52", "52"],
      "location": "s37from52to52",
      "content": "يَقُولُ أَإِنَّكَ لَمِنَ الْمُصَدِّقِينَ ﴿52﴾"
    },
    {
      "id": 53,
      "color": "#b1acca",
      "from-to": ["53", "53"],
      "location": "s37from53to53",
      "content": "أَإِذَا مِتْنَا وَكُنَّا تُرَابًا وَعِظَامًا أَإِنَّا لَمَدِينُونَ ﴿53﴾"
    },
    {
      "id": 54,
      "color": "#b1acca",
      "from-to": ["54", "54"],
      "location": "s37from54to54",
      "content": "قَالَ هَلْ أَنْتُمْ مُطَّلِعُونَ ﴿54﴾"
    },
    {
      "id": 55,
      "color": "#b1acca",
      "from-to": ["55", "55"],
      "location": "s37from55to55",
      "content": "فَاطَّلَعَ فَرَآهُ فِي سَوَاءِ الْجَحِيمِ ﴿55﴾"
    },
    {
      "id": 56,
      "color": "#b1acca",
      "from-to": ["56", "56"],
      "location": "s37from56to56",
      "content": "قَالَ تَاللَّهِ إِنْ كِدْتَ لَتُرْدِينِ ﴿56﴾"
    },
    {
      "id": 57,
      "color": "#b1acca",
      "from-to": ["57", "57"],
      "location": "s37from57to57",
      "content": "وَلَوْلَا نِعْمَةُ رَبِّي لَكُنْتُ مِنَ الْمُحْضَرِينَ ﴿57﴾"
    },
    {
      "id": 58,
      "color": "#b1acca",
      "from-to": ["58", "58"],
      "location": "s37from58to58",
      "content": "أَفَمَا نَحْنُ بِمَيِّتِينَ ﴿58﴾"
    },
    {
      "id": 59,
      "color": "#b1acca",
      "from-to": ["59", "59"],
      "location": "s37from59to59",
      "content": "إِلَّا مَوْتَتَنَا الْأُولَىٰ وَمَا نَحْنُ بِمُعَذَّبِينَ ﴿59﴾"
    },
    {
      "id": 60,
      "color": "#b1acca",
      "from-to": ["60", "60"],
      "location": "s37from60to60",
      "content": "إِنَّ هَٰذَا لَهُوَ الْفَوْزُ الْعَظِيمُ ﴿60﴾"
    },
    {
      "id": 61,
      "color": "#b1acca",
      "from-to": ["61", "61"],
      "location": "s37from61to61",
      "content": "لِمِثْلِ هَٰذَا فَلْيَعْمَلِ الْعَامِلُونَ ﴿61﴾"
    },
    {
      "id": 62,
      "color": "#f6dfe9",
      "from-to": ["62", "62"],
      "location": "s37from62to62",
      "content": "أَذَٰلِكَ خَيْرٌ نُزُلًا أَمْ شَجَرَةُ الزَّقُّومِ ﴿62﴾"
    },
    {
      "id": 63,
      "color": "#f6dfe9",
      "from-to": ["63", "63"],
      "location": "s37from63to63",
      "content": "إِنَّا جَعَلْنَاهَا فِتْنَةً لِلظَّالِمِينَ ﴿63﴾"
    },
    {
      "id": 64,
      "color": "#f6dfe9",
      "from-to": ["64", "64"],
      "location": "s37from64to64",
      "content": "إِنَّهَا شَجَرَةٌ تَخْرُجُ فِي أَصْلِ الْجَحِيمِ ﴿64﴾"
    },
    {
      "id": 65,
      "color": "#f6dfe9",
      "from-to": ["65", "65"],
      "location": "s37from65to65",
      "content": "طَلْعُهَا كَأَنَّهُ رُءُوسُ الشَّيَاطِينِ ﴿65﴾"
    },
    {
      "id": 66,
      "color": "#f6dfe9",
      "from-to": ["66", "66"],
      "location": "s37from66to66",
      "content": "فَإِنَّهُمْ لَآكِلُونَ مِنْهَا فَمَالِئُونَ مِنْهَا الْبُطُونَ ﴿66﴾"
    },
    {
      "id": 67,
      "color": "#f6dfe9",
      "from-to": ["67", "67"],
